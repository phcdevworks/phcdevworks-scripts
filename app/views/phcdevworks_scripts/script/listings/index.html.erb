<p id="notice"><%= notice %></p>

<h1>Script Listings</h1>

<table>
  <thead>
    <tr>
      <th>Listing title</th>
      <th>Listing description</th>
      <th>Listing script source</th>
      <th>Listing script website</th>
      <th>Listing script repo</th>
      <th>Listing script initial release</th>
      <th>Listing script lastest release</th>
      <th>Listing script beta release</th>
      <th>Listing script lastest release cdn</th>
      <th>Listing script status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @script_listings.each do |script_listing| %>
      <tr>
        <td><%= script_listing.listing_title %></td>
        <td><%= script_listing.listing_description %></td>
        <td><%= script_listing.listing_script_source %></td>
        <td><%= script_listing.listing_script_website %></td>
        <td><%= script_listing.listing_script_repo %></td>
        <td><%= script_listing.listing_script_initial_release %></td>
        <td><%= script_listing.listing_script_lastest_release %></td>
        <td><%= script_listing.listing_script_beta_release %></td>
        <td><%= script_listing.listing_script_lastest_release_cdn %></td>
        <td><%= script_listing.listing_script_status %></td>
        <td><%= link_to 'Show', script_listing %></td>
        <td><%= link_to 'Edit', edit_script_listing_path(script_listing) %></td>
        <td><%= link_to 'Destroy', script_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script Listing', new_script_listing_path %>
